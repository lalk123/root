#include <stdio.h>

int main()
{
    int a,b,c,d;
    printf("enter the number");
    scanf("%d%d%d",&a,&b,&c);
    d=b*b-4*a*c;
    printf("the discriminat is :%d",d);
    if(d>0)
    {
        printf(" two +ve root");
    }
    else if(d==0)
    {
        printf("one root");
    }
    else{
        
    printf(" neg root ");
    }

    return 0;
}

